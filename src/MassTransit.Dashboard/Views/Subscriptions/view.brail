<table>
    <caption>A list of subscriptions managed by MassTransit</caption>
	<tr>
	    <th>Address</th>
	    <th>Message</th>
	</tr>
  <% for subscription in subscriptions: %>
	<tr><td>${subscription.EndpointUri}</td><td>${subscription.MessageName}</td></tr>
  <% end %>
</table>

<% component capturefor, { @id : "title" }: %>
<h1>Subscriptions</h1>
<% end %> 
